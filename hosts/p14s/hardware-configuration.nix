  };

  fileSystems."/home" = {
    device = "/dev/disk/by-uuid/0162f1f4-7e50-4fda-9b96-e4d5f2a57db3";
    fsType = "ext4";
  };

  swapDevices = [
    { device = "/dev/disk/by-uuid/76bbe115-e305-4c6f-a424-80b0728a7408"; }
