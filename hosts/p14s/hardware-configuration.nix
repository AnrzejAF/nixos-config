  boot.extraModulePackages = [ ];
  boot.resumeDevice = "/dev/disk/by-uuid/76bbe115-e305-4c6f-a424-80b0728a7408";

  fileSystems."/" = {
    device = "/dev/disk/by-uuid/0626e745-4eb1-475e-959f-acf3f31d5d66";
    fsType = "ext4";
  };

  fileSystems."/boot" = {
    device = "/dev/disk/by-uuid/2C24-2B09";
    fsType = "vfat";
    options = [
      "fmask=0077"
      "dmask=0077"
